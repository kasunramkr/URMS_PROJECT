<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="URMS\AppBundle\Entity\Module" table="module">
    <id name="moduleId" type="string" column="Module_Id" length="20">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="Name" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="coordinator" type="string" column="Coordinator" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="semesterBatch" type="string" column="Semester_Batch" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
