<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="URMS\AppBundle\Entity\Admin" table="admin">
    <indexes>
      <index name="User_Id" columns="User_Id"/>
    </indexes>
    <id name="user" association-key="true"/>
    <field name="name" type="string" column="Name" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address" type="string" column="Address" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="contactNo" type="integer" column="Contact_No" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nicNo" type="string" column="NIC_No" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-one field="user" target-entity="Person" fetch="LAZY">
      <join-columns>
        <join-column name="User_Id" referenced-column-name="User_Id"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
